<%- include('./base/header.ejs') %>
    <%- include('./base/nav.ejs') %>
        <div class="container">
            <div class="alert text-center bg-primary my-3" style="color: white">
                <h5>Edit Student</h5>
            </div>
        </div>
        <div class="container">
        <div class="row">
            <div class="col"></div>
            <div class="col">
                <form action="/edit/student/<%=student._id %>" method="post">
        <div>
            <label class="from-lable">RollNo</label>
            <input type="text" class="form-control" name="RollNo" value="<%= student.RollNo %>"></input>
        </div>
        <div>
            <label class="from-lable">StudentName</label>
            <input type="text" class="form-control" name="StudentName" value="<%= student.StudentName %>"></input>
        </div>
       
        <div>
            <label class="from-lable">FatherName</label>
            <input type="text" class="form-control" name="FatherName" value="<%= student.FatherName %>"></input>
        </div>
        <div>
            <label class="from-lable">MotherName</label>
            <input type="text" class="form-control" name="MotherName" value="<%= student.MotherName %>"></input>
        </div>
        <div>
            <label class="from-lable">Course</label>
            <input type="text" class="form-control" name="Course" value="<%= student.Course %>"></input>
        </div>
        
        <div>
            <label class="from-lable">Branch</label>
            <input type="text" class="form-control" name="Branch" value="<%= student.Branch %>"></input>
        </div>
        
        <div>
            <label class="from-lable">YearOfAdmission</label>
            <input type="text" class="form-control" name="YearOfAdmission" value="<%= student.YearOfAdmission %>"></input>
        </div>
        
        <input type="submit" value="Edit Student" class="btn btn-warning w-100 my-3"></input>
        
        </form>
            </div>
            <div class="col"></div>

        </div>
        <%- include('./base/footer.ejs') %>